import java.util.Scanner;

public class Number {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        String a = sc.nextLine();
        String b = sc.nextLine();
        String c = a + " " + b;
        int d = c.length();
        System.out.println (c + "\n" + d);

        sc.close();
    }
}
